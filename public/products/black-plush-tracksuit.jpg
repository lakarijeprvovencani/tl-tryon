<script>
(function () {
  function cleanThankYou() {
    document.querySelectorAll('.jdgm-write-review-modal__form--thank-you').forEach(function(form){
      // 1) Dozvoli samo PRVI title (Hvala na vašoj recenziji!)
      var titles = form.querySelectorAll('.jdgm-write-review-modal__title');
      titles.forEach(function(el, i){ if (i > 0) el.style.display = 'none'; });

      // 2) Dozvoli samo PRVA DVA intro pasusa
      var intros = form.querySelectorAll('.jdgm-write-review-modal__intro');
      intros.forEach(function(el, i){ if (i > 1) el.style.display = 'none'; });

      // 3) Sigurnosno – sakrij sve kontenjere sa zvezdicama/“share review”
      form.querySelectorAll('.jdgm-write-review-modal__stars, [role="group"][aria-label="Rating"]').forEach(function(el){
        el.style.display = 'none';
      });
    });
  }

  // Pokreni kad se modal prikaže i na svaku promenu u DOM-u (jer se učitava dinamički)
  document.addEventListener('DOMContentLoaded', cleanThankYou);
  document.addEventListener('click', function(){ setTimeout(cleanThankYou, 80); });

  new MutationObserver(function(){ cleanThankYou(); })
    .observe(document.body, { childList: true, subtree: true });
})();
</script>
