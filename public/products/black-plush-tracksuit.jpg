<script>
(function fixRatingLabelsForce(){
  function normalizeText(s){
    if(!s) return "";
    return s
      .replace(/\u00A0/g, " ")   // NBSP -> space
      .replace(/\s+/g, " ")      // višestruki razmaci
      .trim();
  }

  function fix(){
    // Traži sve potencijalne labele u modalu i widgetu
    const candidates = document.querySelectorAll(
      '.jdgm-write-review-modal *, .jdgm-widget *'
    );

    candidates.forEach(el=>{
      if (!el || el.nodeType !== 1) return;

      // 1) Probaj direktnu zamenu teksta
      const t = normalizeText(el.textContent);
      if (t === 'Лош')   { el.textContent = 'Loš';   return; }
      if (t === 'Супер') { el.textContent = 'Super'; return; }

      // 2) Ako je tekst “prazan”, moguće da je pseudo-element.
      // U tom slučaju ubacićemo naš overlay:
      const style = getComputedStyle(el, '::after');
      const afterContent = style && style.content && style.content.replace(/(^")|("$)/g,'');
      if (afterContent === 'Лош') {
        el.style.position = 'relative';
        el.style.color = 'transparent';
        if (!el.querySelector('.tl-fix-after')) {
          const s = document.createElement('span');
          s.className = 'tl-fix-after';
          s.textContent = 'Loš';
          Object.assign(s.style, {position:'absolute',left:0,top:0,color:'inherit'});
          el.appendChild(s);
        }
      }
      if (afterContent === 'Супер') {
        el.style.position = 'relative';
        el.style.color = 'transparent';
        if (!el.querySelector('.tl-fix-after')) {
          const s = document.createElement('span');
          s.className = 'tl-fix-after';
          s.textContent = 'Super';
          Object.assign(s.style, {position:'absolute',left:0,top:0,color:'inherit'});
          el.appendChild(s);
        }
      }
    });
  }

  document.addEventListener('DOMContentLoaded', fix);
  new MutationObserver(fix).observe(document.body, { childList:true, subtree:true, characterData:true });
  // kada korisnik otvara modal
  document.addEventListener('click', () => setTimeout(fix, 150));
})();
</script>
<script>
(function hardReplaceCyrillicLabels() {
  // zameni i u običnim tekst-čvorovima, svuda na stranici
  function sweep() {
    try {
      const walker = document.createTreeWalker(document.body, NodeFilter.SHOW_TEXT, null);
      let n;
      while ((n = walker.nextNode())) {
        if (!n.nodeValue) continue;
        // normalizuj NBSP i višak razmaka
        let txt = n.nodeValue.replace(/\u00A0/g, ' ');
        let changed = false;

        if (/\bЛош\b/.test(txt)) { txt = txt.replace(/\bЛош\b/g, 'Loš'); changed = true; }
        if (/\bСупер\b/.test(txt)) { txt = txt.replace(/\bСупер\b/g, 'Super'); changed = true; }

        // možeš dodati i ostale mape ovde po istom šablonu
        // if (/\bКако бисте оценили овај производ\?\b/.test(txt)) { ... }

        if (changed) n.nodeValue = txt;
      }
    } catch (e) { /* no-op */ }
  }

  // prvi prolaz
  document.addEventListener('DOMContentLoaded', sweep);

  // posmatraj dinamičke izmene (modal se crta kasnije)
  const mo = new MutationObserver(sweep);
  mo.observe(document.body, { childList: true, subtree: true, characterData: true });

  // dodatno posle klikova (otvaranje modala)
  document.addEventListener('click', () => setTimeout(sweep, 150));
})();
</script>
