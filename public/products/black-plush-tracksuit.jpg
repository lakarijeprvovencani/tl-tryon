</style>

<script>
(function () {
  function addConfirmBtn(){
    document.querySelectorAll('.jdgm-write-review-modal__form--thank-you').forEach(function(form){
      if (form.querySelector('.tl-thanks-close')) return; // ne dupliraj

      // Napravi dugme
      var btn = document.createElement('button');
      btn.type = 'button';
      btn.className = 'tl-thanks-close';
      btn.textContent = 'Potvrdi';

      // Klik = zatvori modal kao da je kliknut X
      btn.addEventListener('click', function(){
        var modal = form.closest('.jdgm-write-review-modal');
        var closeBtn = modal ? modal.querySelector('.jdgm-write-review-modal__close-btn') : null;

        if (closeBtn) {
          closeBtn.click(); // najčistije – koristi njihov handler
        } else if (modal) {
          // fallback, u slučaju da X ne postoji iz nekog razloga
          modal.remove();
          document.body.style.overflow = '';
        }
      });

      form.appendChild(btn);
    });
  }

  // pokreni i osiguraj se kroz mutation observer
  document.addEventListener('DOMContentLoaded', addConfirmBtn);
  document.addEventListener('click', function(){ setTimeout(addConfirmBtn, 80); });
  new MutationObserver(function(){ addConfirmBtn(); })
    .observe(document.body, {childList:true, subtree:true});
})();
</script>

