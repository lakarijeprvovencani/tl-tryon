<script>
(function() {
  const MAP = {
    "О вама": "O vama",
    "Молимо реците нам више о себи.": "Molimo recite nam više o sebi.",
    "Имејл адреса": "Email adresa",
    "Ваша имејл адреса": "Vaša email adresa",
    "Обрађујемо вашу приватност.": "Obrađujemo vašu privatnost.",
    "Прикажи име": "Prikaži ime",
    "Анонимна рецензија": "Anonimna recenzija",
    "обавезно": "obavezno",
    "Молимо унесите важећу имејл адресу": "Molimo unesite važeću email adresu"
  };

  function translateText(node) {
    if (node.nodeType === 3) {
      let text = node.nodeValue.trim();
      for (const [sr, lat] of Object.entries(MAP)) {
        if (text.includes(sr)) {
          text = text.replace(sr, lat);
        }
      }
      node.nodeValue = text;
    } else if (node.nodeType === 1) {
      node.childNodes.forEach(translateText);
      if (node.placeholder) {
        for (const [sr, lat] of Object.entries(MAP)) {
          if (node.placeholder.includes(sr)) {
            node.placeholder = node.placeholder.replace(sr, lat);
          }
        }
      }
    }
  }

  function run() {
    document.querySelectorAll('.jdgm-write-review-modal, .jdgm-write-review-modal *').forEach(el => {
      translateText(el);
    });
  }

  document.addEventListener('DOMContentLoaded', run);
  new MutationObserver(run).observe(document.body, { childList: true, subtree: true });
})();
</script>
