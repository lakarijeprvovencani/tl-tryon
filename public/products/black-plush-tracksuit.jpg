<script>
(function fixShareMediaTexts(){
  function run(){
    const page = document.querySelector('.jdgm-write-review-modal__page--share-media.current-page');
    if (!page) return;

    // Naslov
    const title = page.querySelector('.jdgm-write-review-modal__title');
    if (title) title.textContent = 'Podeli sliku';

    // Opis ispod naslova
    const intro = page.querySelector('.jdgm-write-review-modal__intro');
    if (intro) intro.textContent = 'Otpremite fotografiju da biste podržali svoju recenziju.';

    // Prompt u upload boxu
    const prompt = page.querySelector('.jdgm-write-review-modal__media-upload-prompt');
    if (prompt) prompt.innerHTML = '<b>Kliknite da otpremite</b> ili prevucite i otpustite';

    // aria-label na file inputu
    const input = page.querySelector('.jdgm-write-review-modal__media-input');
    if (input) input.setAttribute('aria-label','Podeli sliku');
  }

  // pokreni na load + prati promene (jer se modal menja kroz “slajdove”)
  document.addEventListener('DOMContentLoaded', run);
  new MutationObserver(run).observe(document.body, {childList:true, subtree:true});
})();
</script>
