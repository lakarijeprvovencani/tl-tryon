<script>
(function() {
  function fixThankYou() {
    document.querySelectorAll('.jdgm-write-review-modal__form--thank-you').forEach(el => {
      el.querySelectorAll('*').forEach(node => {
        if (node.childNodes) {
          node.childNodes.forEach(n => {
            if (n.nodeType === 3) {
              let txt = n.nodeValue.trim();

              if (txt.includes("Хвала на вашој рецензији")) {
                n.nodeValue = "Hvala na vašoj recenziji!";
              }
              if (txt.includes("Обрађујемо је и ускоро ће се појавити у продавници")) {
                n.nodeValue = "Obrađujemo je i uskoro će se pojaviti u prodavnici.";
              }
              if (txt.includes("Молимо вас да потврдите своју е-пошту")) {
                n.nodeValue = "Molimo vas da potvrdite svoj email klikom na link koji smo vam poslali. Ovo nam pomaže da održavamo autentične recenzije.";
              }
              if (txt.includes("Да ли бисте волели да поделите своје искуство куповине са нама")) {
                n.nodeValue = "Da li biste voleli da podelite svoje iskustvo kupovine sa nama?";
              }
              if (txt.includes("Ценимо ваше мишљење")) {
                n.nodeValue = "Cenimo vaše mišljenje i koristimo ga za poboljšanje. Molimo vas da podelite bilo kakve komentare ili predloge.";
              }
            }
          });
        }
      });
    });
  }

  document.addEventListener('DOMContentLoaded', fixThankYou);
  new MutationObserver(fixThankYou).observe(document.body, { childList: true, subtree: true, characterData: true });
})();
</script>
